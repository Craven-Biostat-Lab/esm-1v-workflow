# HTC Submit File

# Provide HTCondor with the name of the Docker container
container_image = model1v1.sif
universe = container

+WantFlocking = true # UW pools
+WantGlideIn = true # OSG pool

# Using container's runscript instead of an executable
arguments = --sequences test.fasta --results result.csv

transfer_input_files = osdf:///chtc/staging/sverchkov/model1v1.sif, test.fasta
transfet_output_files = test-results.csv

log = test-job.log
error = test-job.err
output = test-job.out

request_cpus = 1
request_gpus = 1
request_memory = 10GB
request_disk = 10GB

queue
